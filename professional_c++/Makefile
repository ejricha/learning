# Global variables
STD        := -lstdc++ -std=c++17 -pthread
CC         := gcc $(STD)
CXX        := g++ $(STD)
WARN       := -Wall -Wextra -Wfatal-errors
CFLAGS     := -fPIC $(WARN) -O2 -g
RM         := rm -f

.PHONY: all
all: c01_code/c01_code.bin c02_code/c02_code.bin c07_code/c07_code.bin c08_code/c08_code.bin c09_code/c09_code.bin c10_code/c10_code.bin c11_code/c11_code.bin c12_code/c12_code.bin c13_code/c13_code.bin c14_code/c14_code.bin c15_code/c15_code.bin c16_code/c16_code.bin c17_code/c17_code.bin c18_code/c18_code.bin c19_code/c19_code.bin c20_code/c20_code.bin c21_code/c21_code.bin c22_code/c22_code.bin c23_code/c23_code.bin c25_code/c25_code.bin c26_code/c26_code.bin c27_code/c27_code.bin c28_code/c28_code.bin c29_code/c29_code.bin c30_code/c30_code.bin

.PHONY: clean
clean:
	$(RM)  c01_code/c01_code.bin c02_code/c02_code.bin c07_code/c07_code.bin c08_code/c08_code.bin c09_code/c09_code.bin c10_code/c10_code.bin c11_code/c11_code.bin c12_code/c12_code.bin c13_code/c13_code.bin c14_code/c14_code.bin c15_code/c15_code.bin c16_code/c16_code.bin c17_code/c17_code.bin c18_code/c18_code.bin c19_code/c19_code.bin c20_code/c20_code.bin c21_code/c21_code.bin c22_code/c22_code.bin c23_code/c23_code.bin c25_code/c25_code.bin c26_code/c26_code.bin c27_code/c27_code.bin c28_code/c28_code.bin c29_code/c29_code.bin c30_code/c30_code.bin

